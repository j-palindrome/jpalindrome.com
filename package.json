{
  "name": "nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:sanity": "cd sanity && sanity dev",
    "build": "next build",
    "start": "npx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "lint": "next lint",
    "build-cloudflare": "npx @cloudflare/next-on-pages@1",
    "schema": "cd sanity && sanity schema extract --enforce-required-fields",
    "typegen": "cd sanity && sanity typegen generate",
    "pull": "git pull && pnpm pull:asemic && pnpm pull:util",
    "push": "pnpm push:asemic && pnpm push:util && git push",
    "pull:util": "git subtree pull --prefix libs/util https://github.com/j-palindrome/util.git main --squash",
    "push:util": "git push https://github.com/j-palindrome/util.git `git subtree split --prefix libs/util main`:main",
    "pull:asemic": "git subtree pull --prefix libs/asemic https://github.com/j-palindrome/asemic.git main --squash",
    "push:asemic": "git push https://github.com/j-palindrome/asemic.git `git subtree split --prefix libs/asemic main`:main"
  },
  "dependencies": {
    "@elemaudio/core": "^4.0.1",
    "@portabletext/react": "^3.2.1",
    "@react-spring/web": "^9.7.5",
    "@react-three/drei": "10.0.4",
    "@react-three/fiber": "9.1.0",
    "@react-three/postprocessing": "^3.0.4",
    "@sanity/image-url": "^1.1.0",
    "@sanity/preview-url-secret": "^2.1.5",
    "@sanity/react-loader": "^1.10.47",
    "@sanity/vision": "^3.79.0",
    "@tailwindcss/postcss": "^4.0.13",
    "@tailwindcss/typography": "^0.5.16",
    "@types/aframe": "^1.2.8",
    "@types/lodash": "^4.17.16",
    "@types/matter-js": "^0.19.8",
    "@types/p5": "^1.7.6",
    "@types/three": "^0.174.0",
    "@webgpu/types": "^0.1.55",
    "aframe": "^1.7.0",
    "animejs": "^3.2.2",
    "autoprefixer": "^10.4.21",
    "framer-motion": "^12.5.0",
    "groq": "^3.79.0",
    "hydra-synth": "^1.3.29",
    "lodash": "^4.17.21",
    "lucide-react": "^0.479.0",
    "mathjs": "^14.3.1",
    "matter-js": "^0.20.0",
    "next": "15.2.2",
    "next-sanity": "^9.9.0",
    "openai": "^4.87.3",
    "p5": "^1.11.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "pts": "^0.12.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reactive-frames": "^0.2.16",
    "rxjs": "^7.8.2",
    "sanity": "^3.79.0",
    "sanity-image": "^1.0.0",
    "sharp": "^0.33.5",
    "simplex-noise": "^4.0.3",
    "styled-components": "^6.1.15",
    "three": "^0.174.0",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.10",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.22.0",
    "eslint-config-next": "15.2.2",
    "npm": "^11.2.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "wrangler": "^3.114.1"
  }
}
